definition
	CLUSTER[at0000] matches {    -- Test metadata for Leonel
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Patient deactivation
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0006,    -- Patient deceased
							at0007,    -- Patient no longer wishes to participate
							at0008]    -- Other
						}
					}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Patient deactivation
				value matches {
					0|[local::at0003],
					1|[local::at0004],
					999|[local::at0005]
				}
			}
		}
	}

